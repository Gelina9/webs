		<script>
		//图片轮播方法
		function newMySwiper(a){
				var mySwiper = new Swiper(a,{
				autoplay:3000,
				pagination:".swiper-pagination",
				loop:true
			})
		}
//		主页轮播图片高度设置
		var wrapperimg=function(){
			var wrapper=$("#wrapperimg1");
			wrapper.parent().css({
				"height":"100%",
			})
			var h=wrapper.find(".swiper-slide").height();
			var ph=wrapper.find("img").height();
			if(h>ph){
				wrapper.find("img").css({
					"height":"100%",
				})
			}
			wrapper.find(".swiper-slide").css({
				"overflow":"hidden",
			})
		}
		//	遮罩层z-index:10;
		function shade0227(className){
			var shade='<div class="shade0227"></div>';
			$(className).append(shade);
			$("body").css({"overflow":"hidden","height":"100%"});
		}
		shade0227("body");
//		关闭遮罩层
		$(".ad-down-btn,.ad-up-close").on("click",function(){
			$(this).parents(".advertising-popups").hide();
			$(".shade0227").remove();
			$("body").css({"overflow":"auto"});
			$(".fbox,.fswrep").css({"overflow":"hidden","height":"auto"});
//			主页轮播开始
			newMySwiper("#wrapperimg2");
		})
		$(".shade0227").on("click",function(){
			$(".advertising-popups").hide();
			$(".shade0227").remove();
			$("body").css({"overflow":"auto"});
			$(".fbox,.fswrep").css({"overflow":"hidden","height":"auto"});
//			主页轮播开始
			newMySwiper("#wrapperimg2");
		})
	</script>
    <script type="text/javascript">
    //绑定会员图标全屏移动
    var movebtn=function(){
    	var x_start,y_start;//开始xy坐标
	    var x_move,y_move;//移动xy坐标
	    var x_end,y_end;//结束xy坐标
	    var left_start,top_start;//保存对象的left，top值
	    var objw,objh;//对象的宽高
	    var winw,winh;//屏幕的宽高
	    var id=document.getElementById("id");
	    id.addEventListener("touchstart",function(e){
	    	event.preventDefault()
	    	x_start=e.touches[0].pageX;
	    	y_start=e.touches[0].pageY;
	    	right_start=$("#id").css("right");
	    	bottom_start=$("#id").css("bottom");
	    	objw=$("#id").width();
	    	objh=$("#id").height();
	    	winw=$(window).width();
	    	winh=$(window).height();
	    })
	    id.addEventListener("touchmove",function(e){
	    	event.preventDefault()
	    	x_move=e.touches[0].pageX;
	    	y_move=e.touches[0].pageY;
	    	var nright=parseFloat(x_start)-parseFloat(x_move)+parseFloat(right_start);
	    	var nbottom=parseFloat(y_start)-parseFloat(y_move)+parseFloat(bottom_start);
			if(nright<=0){
				$("#id").css("right","0");
			}else if(nright>winw-objw){
				$("#id").css("right",winw-objw);
			}
			else{
				$("#id").css("right",nright);
			}
	    	if(nbottom<=0){
	    		$("#id").css("bottom","0");
	    	}else if(nbottom>winh-objh){
	    		$("#id").css("bottom",winh-objh);
	    	}else{
	    		$("#id").css("bottom",nbottom);
	    	}
	    })
	    id.addEventListener("touchend",function(e){
	    	event.preventDefault()
	    	x_end=e.changedTouches[0].pageX;
	    	y_end=e.changedTouches[0].pageY;
	    })
//	     阻止浏览器下拉事件
//	     $('body').on('touchmove', function (event) {event.preventDefault();});	
    }  
    //根据屏幕宽度判断根字体大小实现手机分辨率自适应
    var nscript=function(){
	  var docEl = document.documentElement,
	    resizeEvt = 'orientationchange' in window ? 'orientationchange':'resize',
	    recalc = function () {
	      var clientWidth = docEl.clientWidth;
	      var clientHeight = docEl.clientHeight;
	      if (!clientWidth) return;
	      docEl.style.fontSize = 20 * (clientWidth / 376) + 'px';
	    };
	  if (!document.addEventListener) return;
	  window.addEventListener(resizeEvt, recalc, false);
	  document.addEventListener('DOMContentLoaded', recalc, false);
	}
    
	movebtn();
	nscript();
	$(function(){
	//		去除微信长按页面弹出"在浏览器打开"
//		document.addEventListener('touchstart', function(e) {
//		    e.preventDefault();
//		}, false);
//		点击毛玻璃效果
		 function Wglass(className){
		 	return $(className).on({
			 		touchstart:function(){
					$(this).addClass("maoboli")
				},
				touchend:function(){
					$(this).removeClass("maoboli")
				}
		 	})
		 }
		 
		 Wglass(".fswrep-menusmall_img");
		 Wglass(".fswrep-menubig_a");
		 
		 
		//解决9格宫出现1像素问题
		var nine=function(){
			//	9格宫自动补齐
			var liLenth=$(".fswrep-menusmall").find("li").length;
			var liNull='<li class="fswrep-menusmall_item"><span class="fswrep-menusmall_icon"></span><p class="fswrep-menusmall_text"></p></li>';
			var liNeed=liLenth%3;
			if(liNeed!=0){
				var newliNeed=3-liNeed;
				for(var i=0;i<newliNeed;i++){
					$(".fswrep-menusmall ul").append(liNull);
				}
			}
			
			var i=1;
			var num=$(".fswrep-menusmall ul li").length;
			for(i;i<=num;i++){
				$(".fswrep-menusmall ul li").eq(3*i-1).css({
					"border-right":"none",
					"width": "6.25rem",
				})
			}
			
			
		}
		nine();
	})
    </script>
