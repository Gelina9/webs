<script type="text/javascript">
	$(function(){
//		..ui-seat-wrap
		var stHeight=$('.ui-relative').outerHeight(true);
		var sfHeight=$('.ui-footer-box').outerHeight(true);
		var scHeight=$("body").height();
		var sbHeight=scHeight-sfHeight-stHeight;
		$(".ui-seat-wrap").css({"height":sbHeight,"overflow":"auto",})
//		alert(sbHeight)
	})
</script>
<script type="text/javascript">
//	换一换弹窗
	$(function(){
		$(".list-btn-way").on("click",function(e){
			e.stopPropagation();
			$(".cinema-list-else").show();
			$(".backdrop").addClass("visible active")
			var liLength=$(".schedule ul li").length;
		if(liLength>3){
			var bcHeight=$(".mod_li").outerHeight(true);
			var newHeightT=bcHeight*4-bcHeight/2+50;
			var newHeight=bcHeight*4-bcHeight/2;
			$(".cinema-list-else").css({"height":newHeightT,});
			$(".schedule").css({"height":newHeight,"overflow":"auto",});
			}
		})
		$(".backdrop").on("click",function(e){
			e.stopPropagation();
			$(".cinema-list-else").hide();
			$(".backdrop").removeClass("visible active");
		})

		$(".ui_price").click(function(e) {
			e.stopPropagation();
			$(this).next().slideToggle();
			$(".ui_price").toggleClass("ui_point");
		});
		
	})
</script>
<script>
    //选座
    $(".ui-seat-active").bind("click",function(){
        if(($(".ui-seat-selected").length + $(".ui-member-seat-selected").length) < 8){
            if(!$(this).hasClass("ui-seat-selected")){
                $(this).addClass("ui-seat-selected");
                if($(this).hasClass("ui-seat-selected")){
                	$(".ui-selectSeat-tip").hide();
                    $(".ui-selected-seat-box").show();
                    $(".ion-close-circled").show();
                    $("<span class='ui-user-submit'>"+ $(this).parent("p").index() + "排" + ($(this).index()+1) + "座</span>").attr("id",$(this).parent("p").index() + "-" + ($(this).index()+1)).appendTo($(".ui-selected-seat-box"))
                };

                //请选座按钮
                $(".ui-submit").removeClass("ui-btn-disabled").addClass("ui-btn-00a1e5").text("确认");
                $(".ui-btn-00a1e5").on("click",function(){
                    location.href='goods.html';
                })
            }
            else{
                if($(this).hasClass("ui-seat-selected")){
                    $(this).toggleClass("ui-seat-selected");
                    $("#" + $(this).parent("p").index() + "-" + ($(this).index()+1)).remove();
                }
                if($(".ui-seat-selected").length < 1){
                    $(".ui-selected-seat-box").hide();
                    $(".ui-selectSeat-tip").show();
                }
            }
        }
        else{
            if($(this).hasClass("ui-seat-selected")){
                $(this).toggleClass("ui-seat-selected");
                $("#" + $(this).parent("p").index() + "-" + ($(this).index()+1)).remove();
            }
          //最多可选座位提示
       		layer.alert('最多可选8个座位!',{
				closeBtn : 0,
				title : '',
				shadeClose : true,
				shade : 0.5,
			});
			$(".layui-layer-btn").addClass('ui-layer-btn');
        }
    });
    


    var app = angular.module("myApp",["ionic"]);
