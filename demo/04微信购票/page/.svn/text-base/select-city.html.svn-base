<script>
	$(function(){
		var cityBtn=function(){
			var me=$(".sel-city");
			var meChildScl=me.find(".sel-city-left");
			var meChildScr=me.find(".sel-city-right");
			meChildScl.children("ul").children("li").eq(0).addClass("is-active")
			meChildScl.children("ul").children("li").on("click",function(){
//				省份选中变色
				$(this).addClass("is-active").siblings().removeClass("is-active");	
			})
			meChildScl.find("li").each(function(index){
				var thisVal=$(this).text();
				if(thisVal.length>4){
					$(this).addClass("is-font5");
				}
			})
			//点击反馈颜色
			meChildScr.find("li").on("click on-drag on-hold on-tap",function(){
			  		$(this).css({
		    		"background-color":"#f2f2f2",
		    	})
			})
		}
		cityBtn()
	})
</script>
<script>
	//输入框
	$("input").bind('input propertychange',function(){
		if($(this).val().length > 0){
			$(this).nextAll(".ion-close-circled").show();
			$(this).parents(".search_sel_city").addClass("select-cinema-box");
			$(".sel_city_box").hide();
			$(".search-new").show();
		}
		if($(this).val().length < 1){
			$(this).nextAll(".ion-close-circled").hide();
			$(this).parents(".search_sel_city").removeClass("select-cinema-box");
			$(".sel_city_box").show();
			$(".search-new").hide();
		}
	});
	$(".ion-close-circled").bind('click',function(){
		$(this).hide();
//		$(".mod_search").value("");
		$(this).prev('.mod_search').val('');
		$(".sel_city_box").show();
		$(".search-new").hide();
		$(this).parents(".search_sel_city").removeClass("select-cinema-box");
	});
	$(".ion-close-btn").bind('click',function(){
		$(".ion-close-circled").hide();
		$(".mod_search").val('');
		$(".sel_city_box").show();
		$(".search-new").hide();
		$(this).parents(".search_sel_city").removeClass("select-cinema-box");
	});
</script>
<script type="text/javascript">
	$(function(){
//		选择城市左边框根据页面补齐
		var bodyStyle=function(){
			var currentCity=$(".current-city").outerHeight(true);
			var recentlyVisited=$(".recently-visited").outerHeight(true);
			var searchSelCity=$(".search_sel_city").outerHeight(true);
			var selCity=$(".sel-city").outerHeight(true);
			var cH=$("html").height()
			var selCityRight=cH-searchSelCity-recentlyVisited-currentCity;
			if(selCity>selCityRight){
				$(".sel-city-right").height(selCity);
			}else{
				$(".sel-city-right").height(selCityRight);
			}
		}
		bodyStyle()
	})
</script>
