<script>
	$(".imgbtn12,.ui-goods-title").on("click on-drag on-hold on-tap",function(){
		  		$(this).parents(".item-thumbnail-left").css({
	    		"background-color":"#f2f2f2",
	    	})
	})


	$("#finish-sh").on("click",function(){
		$("#shoppingcar-tc").show();
		$("#shoppingcar-tc-bj").hide();

	})
//卖品种类切换
	$(".top-nemu").on("click",".nemu",function(e){
		$(this).addClass("active").siblings(".nemu").removeClass("active");
	})
//点击购物车	
	$(".db-shoppingcar").on("click",function(){
		$(".shade").show();
		$("#shoppingcar-tc").show();
		isheight();
	})
//购物车弹窗编辑
	$(".shoppingcar-tc .title1 em").on("click",function(){
		var me=$(this);
		me.parents(".shoppingcar-tc").hide();
		me.parents(".shoppingcar-tc").siblings(".shade").hide();
	})
	$("#editor-sh").on("click",function(){
		$("#shoppingcar-tc").hide();
		$("#shoppingcar-tc-bj").show();
		$(".shade").off("click");
	})
	$("#shoppingcar-tc-bj .x").on("click touchend",function(){
		var me=$(this);
		me.addClass("rx");
		var datanum=me.parents(".name").attr("data-nums");
		me.parents(".name").remove();
		spnum=spnum-1;
		isheight();
		$("#wpnum").text(spnum);
		if(spnum==0){$("#hehe1").show();$(".g-price").html("¥0.00")
		}
	})
	$("#shoppingcar-tc-bj .x").on("touchstart",function(){
		var me=$(this);
		me.addClass("rx");
	})
	
	$(".shade").on("click",function(){
		var me=$(this);
		me.hide();
		$("#shoppingcar-tc").hide();
		$("#shoppingcar-tc-bj").hide();
		
	})
	
	//弹窗高度
	var spnum=$("#wpnum").text();
	function isheight(){
		var topH=$(".shoppingcar-tc .title1").outerHeight(true);
		var footH=$(".shoppingcar-tc .foot").outerHeight(true);
		var wpnum=spnum;
		var ch=$("body").outerHeight(true);
		var shownums=parseInt(ch/44/2);
		var newshownums=parseInt(ch/2);
		if(wpnum>shownums){wpnum=shownums}
		if(wpnum<shownums){wpnum=wpnum}
		var bodyH=wpnum*topH;
		$(".bodyH").css({"height":newshownums});
	}
	isheight();
</script>
