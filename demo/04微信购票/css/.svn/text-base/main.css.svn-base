/* 结算 */
.ui-account-film{ border-bottom: 1px dashed #adadad;}
.bar-header.ui-bg-ff9900{ background: #fffccc; border-bottom: 1px solid #d9ae6d;}
.bar-footer.ui-bg-f0f0f0{ background: #f0f0f0; height: auto; padding: 5px 0; border-top: 1px solid #eee;}
.ui-account-film-name{ margin: 0px auto 5px auto; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; padding-top:10px;}
.ui-account-film-name span{ display: inline-block;}
.ui-account-film-info{ margin: 0 auto; padding-bottom: 10px;}
.ui-account-film-info p{ margin: 0;}
.ui-seat-btn{ float:left;padding: 2px 0; border: 1px solid #bbb;  border-radius: 3px;  font-size: 12px;  color: #555; background: #f0f0f0; min-width:70px; display: inline-block; text-align: center; margin: 0 5px 5px 0;}
.ui-account-seat{ padding: 10px 0 10px 0; border-bottom: 1px solid #e3e3e3; font-size:14px;}
.ui-account-seat ul{ width: 94%; margin: 0 auto;}
.ui-goods-up-box{ background: #fff;}
.ui-goods-up{ margin: 0 auto;}
.ui-box{ border-bottom: 1px solid #e3e3e3; padding: 10px 0;}
.ui-box .ui-input-border{ display: inline-block; width: calc(100% - 85px); font-size: 16px; padding: 0 0 0 5px; border-radius: 3px; border:1px solid #00a1e5;}
.ui-input-notBorder::-webkit-input-placeholder{color:#9aa9af;}
.ui-need-pay{ display: inline-block;}
.ui-account-detail{ margin: 0 10px}
.ui-content{ height: calc(100% - 118px)}
.ui-popup-head{ color: #fff; height: 45px;  line-height: 45px;  text-indent: 10px;  font-size: 18px;}
.ui-del{ font-size: 30px; line-height: 40px;  display: inline-block;  height: 40px;  width: 40px;}
.ui-pay-detail-box{ position: fixed; width: 100%;  bottom: 0;  z-index: 15;  background: #fff; display: none;}
.ui-account-film-price,.ui-goods-down,.ui-goods-up-btn{ display: none;}
.ui-input-del i { position: absolute; right: 0;  padding: 8px 10px;  color: #aaa;  font-size: 22px;  top: 50%;  margin-top: -20px;  display: none;}
.ui-chosen-input { line-height: 1.8rem;  border: 1px solid #ccc;  border-radius:3px;  position: relative; }
.ui-ico {  position: absolute;  right: 10px;  top: 0px;  display: inline-block;  margin: 0;  padding: 0;}
.ui-slideDown { display: inline-block;  width: 24px;  height: 34px;  position: relative;  text-align: center; }
.ui-slideDown-arrow { font-size: 12px;  color: #fff;  position: relative;  text-align: center;  width: 24px;  height: 24px;  border-radius: 2px;  background: #999;  display: inline-block;  margin: 0 auto; top: 5px;}
.ui-slideDown-arrow:after { content: "";  display: inline-block;  border: 0 solid #fff;  border-width: 0 0 2px 2px;  width: 10px;  height: 10px;  -webkit-transform: rotate(-45deg);  -moz-transform: rotate(-45deg);  -ms-transform: rotate(-45deg);  transform: rotate(-45deg);  margin-left: 8px;  position: absolute;  right: 7px;  top: 5px;  z-index: 1;}
.ui-box .ui-chosen-input{padding:10px 5px;font-size: 16px; width:calc(100% - 40px); overflow:hidden; text-overflow:ellipsis; white-space:nowrap; -webkit-box-sizing: border-box;  -moz-box-sizing: border-box;  box-sizing: border-box;}
.ui-box .ui-chosen-input-box .ui-chosen-input{ background:#fff;}
.ui-chosen-input-box{ width: calc(100% - 85px); position: relative; display: inline-block; border:1px solid #ccc; border-radius:3px;}
.ui-chosen-drop { display: none;  width: calc(100% - 80px);  position: absolute; right: 0; top:35px;  z-index: 10;  border-radius: 4px;  -webkit-box-shadow: 0 2px 5px rgba(0,0,0,0.26);  box-shadow: 0 2px 5px rgba(0,0,0,0.26);  background: #fff;}
.ui-chosen-results li { font-size: 16px;  line-height: 24px;  margin: 0;  padding: 6px 5px;  color: #786e55; }
.ui-pop-content .row{ padding: 10px 5px;}
.ui-member-ico{ width:30px; height:20px; display:inline-block; background:url(../image/memberPayIcon.png) no-repeat 0 -4px;}
.ui-arrow-up{ width: 13px;height: 13px;border: 2px solid #ccc;border-width: 0 2px 2px 0px;-webkit-transform: rotate(-135deg);-ms-transform: rotate(-135deg);transform: rotate(-135deg);display: inline-block;position: relative;top: 3px;}
.ui-more-member{ border:1px solid #e3e3e3; border-radius:3px;}
